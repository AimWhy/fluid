INPUT              = ../src/imports
OUTPUT_DIRECTORY   = @DOXYGEN_OUTPUT_DIR@
RECURSIVE          = YES
EXTRACT_ALL        = YES
EXTRACT_PRIVATE    = YES
EXTRACT_STATIC     = YES
EXTRACT_LOCAL_METHODS = YES
FILTER_PATTERNS    = *.qml=doxyqml
FILE_PATTERNS      = *.qml *.cpp *.h
EXTENSION_MAPPING  = qml=C++
ENABLE_PREPROCESSING = YES
HAVE_DOT           = YES
CLASS_DIAGRAMS     = YES
