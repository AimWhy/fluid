{
  "package": {
    "name": "fluid",
    "repo": "ci",
    "subject": "qmlos",
    "desc": "A collection of cross-platform QtQuick components for building fluid and dynamic applications",
    "website_url": "https://github.com/qmlos",
    "issue_tracker_url": "https://github.com/qmlos/fluid",
    "vcs_url": "https://github.com/qmlos/fluid",
    "licenses": ["LGPL-3.0"],
    "labels": ["qt", "qml", "qtquick"]
  },
  "version": {
    "name": "latest_build",
    "desc": "Continuous integration build",
    "released": "@TODAY@",
    "vcs_tag": "@GITREV@",
    "attributes": [
      {"name": "gitrevision", "values": ["@GITREV@"], "type": "string"},
      {"name": "gitdate", "values": ["@GITDATE@"], "type": "date"}
    ],
    "gpgSign": false
  },
  "files": [
    {"includePattern": "build/(fluid-gcc.tar.xz)", "uploadPattern": "$1"},
    {"includePattern": "build/(fluid-clang.tar.xz)", "uploadPattern": "$1"}
  ],
  "publish": true
}
